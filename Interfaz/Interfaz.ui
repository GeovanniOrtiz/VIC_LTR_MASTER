    <string notr="true">/* /////////////////////////////////////////////////////////////////////////////////////////////////

SET APP STYLESHEET - FULL STYLES HERE
DARK THEME - DRACULA COLOR BASED

///////////////////////////////////////////////////////////////////////////////////////////////// */



QWidget{
	color: rgb(221, 221, 221);
	font: 10pt &quot;Segoe UI&quot;;
}

QPushButton:checked {
    background-color: rgb(255,196);
}
QPushButton:unchecked {
    background-color: rgb(33,37,43);
}


/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
	color: #ffffff;
	background-color: rgba(33, 37, 43, 180);
	border: 1px solid rgb(44, 49, 58);
	background-image: none;
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 2px solid rgb(255, 121, 198);
	text-align: left;
	padding-left: 8px;
	margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {	
	background-color: rgb(40, 44, 52);
	border: 1px solid rgb(44, 49, 58);
	padding-left: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {	
	background-color: rgb(33, 37, 43);
}
#topLogo {
	background-color: rgb(33, 37, 43);
	background-image: url(:/images/images/images/Cat_logo.png);
	background-position: centered;
	background-repeat: no-repeat;
}
/*Titulos de la app*/
#titleLeftApp { font:16pt &quot;Segoe UI Semibold&quot;; }
#titleLeftDescription { font: 14pt &quot;Segoe UI &quot;; color: rgb(0, 85, 255 ); }



#user { font:16pt &quot;Segoe UI Semibold&quot;; }
#lbl_user { font: 16pt &quot;Segoe UI Semibold&quot;; color: rgb(255, 233, 51 ); }

/*Modelo del HVAC*/
#model { font:16pt &quot;Segoe UI Semibold&quot;; }
#lbl_model { font: 16pt &quot;Segoe UI Semibold&quot;; color: rgb(0, 85, 255 ); }

/*Modelo del HVAC*/
#mSerial{ font:16pt &quot;Segoe UI Semibold&quot;; }
#lbl_Serial1 { font: 16pt &quot;Segoe UI Semibold&quot;; color: rgb(0, 85, 255 ); }
#lbl_nPiezas { font: 16pt &quot;Segoe UI Semibold&quot;; color: rgb(0, 85, 255 ); }



/* MENUS */
#leftMenuFramee .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 10px solid transparent;
	background-color: transparent;
	text-align: left;
	padding-left: 44px;
}
#leftMenuFramee .QPushButton:hover {
	background-color: rgb(40, 44, 52);
}
#leftMenuFramee .QPushButton:pressed {	
	/*background-color: rgb(255, 233, 51 );*/
	background-color: rgb(0, 170, 255);
	color: rgb(255, 255, 255);
}
#bottomMenu .QPushButton {	
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
	background-color: rgb(40, 44, 52);
}
#bottomMenu .QPushButton:pressed {	
	background-color: rgb(189, 147, 249);
	color: rgb(255, 255, 255);
}
#leftMenuFrame{
	border-top: 3px solid rgb(44, 49, 58);
}

/* Estilos para el botón cuando está checkeado */
#settingsTopBtn.QPushButton:checked {
    background-color: rgb(255,196,0);
}

/* Estilos para el botón cuando no está checkeado */
#settingsTopBtn.QPushButton:unchecked {
    background-color: rgb(33,37,43);
}

/* Toggle Button */
#toggleButton {
	background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 20px solid transparent;
	background-color: rgb(37, 41, 48);
	text-align: left;
	padding-left: 44px;
	color: rgb(113, 126, 149);
}
#toggleButton:hover {
	background-color: rgb(40, 44, 52);
}
#toggleButton:pressed {
	/*background-color: rgb(189, 147, 249);*/
    background-color: rgb(0, 170, 255);
}

/* Estilos para el botón cuando está checkeado */
#toggleButton.QPushButton:checked {
   /*background-color: rgb(255,196,0);*/
	background-color: rgb(0, 170, 255);
}

/* Estilos para el botón cuando no está checkeado */
#toggleButton.QPushButton:unchecked {
    background-color: rgb(33,37,43);
}


/* Title Menu */
#titleRightInfo { padding-left: 10px; }

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Extra Tab */
#extraLeftBox {	
	background-color: rgba(0, 85, 255);
}
#extraTopBg{	
	background-color: rgb(0, 85, 255);
}

/* Icon */
#extraIcon {
	background-position: center;
	background-repeat: no-repeat;
	background-image: url(:/icons/images/icons/icon_settings.png);
}

/* Label */
#extraLabel { color: rgb(255, 255, 255); }

/* Btn Close */
#extraCloseColumnBtn { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
/*#extraCloseColumnBtn:hover { background-color: rgb(196, 161, 249); border-style: solid; border-radius: 4px; }*/
#extraCloseColumnBtn:pressed {background-color: rgb(255, 233, 51 ); border-style: solid; border-radius: 4px; }

/* Extra Content */
#extraContent{
	border-top: 3px solid rgb(40, 44, 52);
}

/* Extra Top Menus */
#extraTopMenu .QPushButton {
background-position: left center;
    background-repeat: no-repeat;
	border: none;
	border-left: 22px solid transparent;
	background-color:transparent;
	text-align: left;
	padding-left: 44px;
}
#extraTopMenu .QPushButton:hover {
	background-color: rgb(40, 44, 52);
}
#extraTopMenu .QPushButton:pressed {	
	background-color: rgb(255, 233, 51 );
	color: rgb(255, 255, 255);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Content App */
#contentTopBg{	
	background-color: rgb(33, 37, 43);
}
#contentBottom{
	border-top: 3px solid rgb(44, 49, 58);
}

/* Top Buttons */
#rightButtons_2 .QPushButton { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#rightButtons_2 .QPushButton:hover { background-color: rgb(44, 49, 57); border-style: solid; border-radius: 4px; }
#rightButtons_2.QPushButton:pressed { background-color: rgb(23, 26, 30); border-style: solid; border-radius: 4px; }

/* Theme Settings */
#extraRightBox { background-color: transparent; /*rgb(44, 44, 52);*/ }
#themeSettingsTopDetail { background-color: rgb(255,196,0); }

/* Bottom Bar */
#bottomBar { background-color: rgb(44, 49, 58); }
#bottomBar QLabel { font-size: 11px; color: rgb(113, 126, 149); padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }

/* CONTENT SETTINGS */
