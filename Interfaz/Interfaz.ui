                       <widget class="QCalendarWidget" name="calendarWidget">
                        <property name="styleSheet">
                         <string notr="true">/* CONTENEDOR PRINCIPAL DEL CALENDARIO */
QCalendarWidget {
    background-color: rgba(30, 30, 30, 180);   /* fondo semi-transparente */
    color: white;
    border: 2px solid rgba(200, 200, 200, 80); /* borde exterior visible */
    border-radius: 10px;
    padding: 5px;
}

/* BARRA DE NAVEGACIÓN (mes y año) */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: rgba(45, 45, 45, 200);
    border-bottom: 1px solid rgba(255, 255, 255, 30);
}

/* BOTONES DE MES Y AÑO (están implementados como QToolButton) */
QCalendarWidget QToolButton {
    background-color: rgba(80, 80, 80, 180);
    color: white;
    font-weight: bold;
    border: 1px solid rgba(255, 255, 255, 30);
    border-radius: 6px;
    padding: 4px 8px;
    qproperty-iconSize: 16px;
}

/* HOVER DE LOS BOTONES */
QCalendarWidget QToolButton:hover {
    background-color: rgba(100, 100, 100, 220);
}

/* OCULTAR INDICADOR DE MENÚ EN BOTONES */
QCalendarWidget QToolButton::menu-indicator {
    image: none;
}

/* MENÚ DESPLEGABLE DE MESES Y AÑOS */
QCalendarWidget QMenu {
    background-color: rgba(40, 40, 40, 220);
    color: white;
    border: 1px solid rgba(255, 255, 255, 50);
    padding: 4px;
    selection-background-color: rgba(70, 130, 180, 180);
    selection-color: white;
}

/* VISTA DE DÍAS */
QCalendarWidget QAbstractItemView {
    background-color: transparent;
    selection-background-color: rgba(70, 130, 180, 180);
    selection-color: white;
    gridline-color: rgba(255, 255, 255, 20);
    font-size: 14px;
}

/* CADA CELDA DE DÍA */
QCalendarWidget QAbstractItemView::item {
    background-color: transparent;
    color: white;
    padding: 5px;
    border-radius: 4px;
}

/* DÍA ACTUAL SELECCIONADO */
QCalendarWidget QAbstractItemView::item:selected {
    background-color: rgba(70, 130, 180, 180);
    color: white;
}

/* DÍA CON HOVER */
QCalendarWidget QAbstractItemView::item:hover {
    background-color: rgba(255, 255, 255, 30);
}

/* DÍAS FUERA DEL MES ACTUAL */
QCalendarWidget QAbstractItemView::item:disabled {
    color: rgba(255, 255, 255, 60);
}
</string>
